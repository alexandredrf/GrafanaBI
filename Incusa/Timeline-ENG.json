          "rawQuery": true,
          "rawSql": "select (select\r\nsum(case when ms.duration > 10 then 10 \r\n\t\t        else ms.duration end) \r\nfrom machines_state ms \r\ninner join machines mach\r\non ms.machine_id = mach.id\r\nwhere mach.description = '$maquina' and state in ('5') and $__timeFilter(time) \r\ngroup by machine_id)/60 as \"Running with P.O\",\r\n(select\r\nsum(case when ms.duration > 10 then 10 \r\n\t\t        else ms.duration end) \r\nfrom machines_state ms \r\ninner join machines mach\r\non ms.machine_id = mach.id\r\nwhere mach.description = '$maquina' and state in ('4') and $__timeFilter(time) \r\ngroup by machine_id)/60 as \"Running without P.O\",\r\n(select\r\nsum(case when ms.duration > 10 then 10 \r\n\t\t        else ms.duration end)   \r\nfrom machines_state ms \r\ninner join machines mach\r\non ms.machine_id = mach.id\r\nwhere mach.description = '$maquina' and state in ('3') and $__timeFilter(time) \r\ngroup by machine_id)/60 as \"Stopped machine\",\r\n(select\r\nsum(case when ms.duration > 10 then 10 \r\n\t\t        else ms.duration end)  \r\nfrom machines_state ms \r\ninner join machines mach\r\non ms.machine_id = mach.id\r\nwhere mach.description = '$maquina' and state in ('2') and $__timeFilter(time) \r\ngroup by machine_id)/60 as \"Idle machine\",\r\n(select\r\nsum(case when ms.duration > 10 then 10 \r\n\t\t        else ms.duration end)  \r\nfrom machines_state ms \r\ninner join machines mach\r\non ms.machine_id = mach.id\r\nwhere mach.description = '$maquina' and state in ('1','0') and $__timeFilter(time) \r\ngroup by machine_id)/60 as \"Offline\",\r\n(select \r\nsum(case when ms.duration > 10 then 10 \r\n\t\t        else ms.duration end)\r\nfrom machines_state ms \r\ninner join machines mach\r\non ms.machine_id = mach.id\r\nwhere mach.description = '$maquina' and $__timeFilter(time) \r\ngroup by machine_id)/60 as \"Total time\" from machines_state ms2 \r\ngroup by 1",
